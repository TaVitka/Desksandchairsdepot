    "@parcel/optimizer-esbuild": "^2.8.3",
    "@parcel/optimizer-svgo": "^2.8.2",
    "@parcel/transformer-inline-string": "^2.8.2",
    "@parcel/transformer-postcss": "^2.8.2",
